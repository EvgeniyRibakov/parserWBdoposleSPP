displayMode == 'd' && activeChat^phone}} <div class=\"chat__text chat__text--gray\"> {^{:activeChat^phone}} </div> {{else ~wbSettings.displayMode == 'm'}} <div class=\"chat__btn-nav chat__btn-nav--phone\" data-link=\"{on callCourier activeChat^phone}class{merge: !activeChat^phone toggle='chat__btn-nav--non-phone'}\"></div> {{/if}} </div> {{else}} <div class=\"chat__text\"> {^{:activeChat^title}} <div class=\"chat__item-fade\"></div> </div> {{/if}} {{else activeChat^name == 'travel'}} <span class=\"chat__logo chat__logo--travel\"></span> <div class=\"chat__text\"> {^{:activeChat^title}} <div class=\"chat__item-fade\"></div> </div> {{else activeChat^name == 'realty'}} <span class=\"chat__logo chat__logo--realty\"></span> <div class=\"chat__text\"> {^{:activeChat^title}} <div class=\"chat__item-fade\"></div> </div> {{else}} <a class=\"chat__seller\" data-link=\"{on 'click' onGoToUrl}{chatAnalytics 'Purchases_Chat_Supplier_T'}href{:activeChat.isC2C ? '/catalog/' + activeChat.goodCard.nmID + '/detail.aspx' : '/seller/' + activeChat^sellerId}\" target=\"_blank\"> <span class=\"chat__logo img-plug\"> {^{if activeChat^goodCard && ~wbSettings.currentLocale != 'by'}} <img src=\"\" data-link=\"src{>~ImageUrl(activeChat.goodCard.nmID, 'c246x328', 1, false, viewFlags)}data-src-pb{>~ImageUrlPb(activeChat.goodCard.nmID, 'c246x328', 1, false, viewFlags)}data-src-pc{urlSiteStaticPb:'i/v3/broken-img/product-icon.svg'}\" width=\"48\" height=\"48\" alt=\"{{:~t('chat.productImage')}}\"> {{else activeChat^hasLogo}} <img src=\"\" data-link=\"src{> ~forSellerLogo(activeChat^sellerId)}data-src-pb{> ~forSellerLogoPb(activeChat^sellerId)}data-src-pc{urlSiteStaticPb:'i/v3/broken-img/shop-icon.svg'}\" width=\"96\" height=\"40\" alt=\"{{:~t('chat.sellerLogo')}}\"> {{else}} <img src=\"{{urlSiteStatic:'i/v3/broken-img/shop-icon.svg'}}\" data-src-pb=\"{{urlSiteStaticPb:'i/v3/broken-img/shop-icon.svg'}}\" width=\"96\" height=\"40\" alt=\"{{:~t('chat.sellerLogo')}}\"> {{/if}} </span> {^{if activeChat^goodCard}} {^{include activeChat^goodCard ~title=activeChat^title ~seller=quotationTransform(activeChat^sellerName) ~isReseller=activeChat^isReseller}} <div class=\"chat__nav-title\"> <div class=\"chat__text\"> {^{if name}} {{>name}} {{else}} {^{:~title}} {{/if}} <div class=\"chat__item-fade\"></div> </div> <div class=\"chat__text chat__text--gray\"> {^{if ~isReseller}} {{:~t('chat.chatWithPurchaser')}} {{else ~seller}} {{:~t('chat.withSeller.chatWithSellerWithValue', { 'seller': ~seller })}} {{else}} {{:~t('chat.withSeller.chatWithSeller')}} {{/if}} <div class=\"chat__item-fade\"></div> </div> </div> {{/include}} {{else}} <div class=\"chat__text\"> {^{:activeChat^title}} <div class=\"chat__item-fade\"></div> </div> {{/if}} </a> {{/if}} </div> {{/if}} </div> <button class=\"chat__btn-close j-close\" data-link=\"{chatAnalytics 'Purchases_Chat_Close'}\" aria-label=\"{{:~t('chat.closeChats')}}\" type=\"button\"></button> </div> {^{if isProductSearchOpen && activeChat^screen === 'products'}} <div class=\"chat__search-product search-product\"> <div class=\"search-product__input input-search input-search--gray\"> <label> <input                                 class=\"j-search-product\" type=\"text\" placeholder=\"{{:~t('chat.searchForProducts')}}\" data-link=\"{:archiveFilters.query:}\" > </label> </div> <button class=\"search-product__close\" type=\"button\" data-link=\"{on toggleSearchProductVisible}\">{{:~t('chat.cancel')}}</button> </div> {{/if}} {^{if activeChat^screen === 'products'}} <div class=\"chat__found-products chat-goods chat-goods--found\" data-link=\"{on 'click' '.js-choose-product' chooseProduct true}\"> <div class=\"chat__found-wrapper\"> {^{if ~root.totalProducts.length > 0}} <ul class=\"chat-goods__list\"> {^{for ~root.totalProducts}} <li class=\"chat-goods__item js-choose-product\"> <div class=\"chat-goods__item-content\"> <div class=\"chat-goods__image img-plug\"> <img src=\"{{:~ImageUrl(code1S, 'c516x688', 1, false, viewFlags)}}\" data-src-pb=\"{{:~ImageUrlPb(code1S, 'c516x688', 1, false, viewFlags)}}\" width=\"51\" height=\"68\" alt=\"{{:~t('chat.productImage')}}\"> </div> <div class=\"chat-goods__info\"> <div class=\"chat-goods__info-top\"> <p class=\"chat-goods__price\">{{priceWithCurrencyV2:price true iso=currency}}</p> {^{if status == \"Refund\" || status == \"ExcludedFromRate\" || status == \"FailedPayment\"}} <p class=\"chat-goods__status chat-goods__status--refund\">{{>trackingStatus}} {{formatDate:lastDate 'dd.MM.yy'}}</p> {{else status == \"Rejected\" || status == \"Purchased\"}} <p class=\"chat-goods__status\">{{>trackingStatus}} {{formatDate:lastDate 'dd.MM.yy'}}</p> {{else status == \"error\"}} <p class=\"chat-goods__status chat-goods__status--refund\">{{>trackingStatus}} {{if !isActive}}{{formatDate:lastDate 'dd.MM.yy'}}{{/if}}</p> {{else status == \"info\"}} <p class=\"chat-goods__status\">{{>trackingStatus}} {{if !isActive}}{{formatDate:lastDate 'dd.MM.yy'}}{{/if}}</p> {{else status == \"warning\"}} <p class=\"chat-goods__status chat-goods__status--warning\">{{>trackingStatus}} {{if !isActive}}{{formatDate:lastDate 'dd.MM.yy'}}{{/if}}</p> {{else status == \"success\"}} <p class=\"chat-goods__status chat-goods__status--ready\">{{>trackingStatus}} {{if !isActive}}{{formatDate:lastDate 'dd.MM.yy'}}{{/if}}</p> {{else trackingStatus && readyToReceiveToday}} <p class=\"chat-goods__status chat-goods__status--ready\">{{>trackingStatus}}</p> {{else trackingStatus}} <p class=\"chat-goods__status chat-goods__status--on-way\">{{>trackingStatus}}</p> {{else}} <p class=\"chat-goods__status chat-goods__status--on-way\">{{:~t('chat.onTheWay')}}</p> {{/if}} </div> <p class=\"chat-goods__info-middle\"> <span class=\"chat-goods__good-name\">{{>brand}}{{if brand && name}} / {{/if}}{{>name}}</span> </p> {{if color || name}} <ul class=\"chat-goods__params\"> {{if color}} <li class=\"chat-goods__param\">{{>color}}</li> {{/if}} {{if size && size != '0'}} <li class=\"chat-goods__param\">{{>size}}</li> {{/if}} </ul> {{/if}} </div> <div class=\"chat-goods__nav-icon\"></div> </div> </li> {{/for}} </ul> {{/if}} {^{if ~root.totalProducts.length == 0}} <div class=\"chat-goods__no-result empty-page\"> {^{if ~root.archiveLoaded}} {^{if ~root.isProductSearchOpen}} <p class=\"chat-goods__no-result-text\">{{:~t('chat.nothingFound')}}</p> {{else}} <div class=\"chat-goods__no-result-icon empty-page__icon\"> <img src=\"{{urlSiteStatic:'i/v3/chat/info-delivery.svg'}}\"> </div> <h1 class=\"chat-goods__no-result-title empty-page__title\">{{:~t('chat.noProductsTitle')}}</h1> <p class=\"empty-page__text\">{{:~t('chat.noProductsSubtitle')}}</p> {{/if}} {{else}} <div class=\"chat-goods__products-loading\"> <div class=\"chat-goods__loader\"></div> <p class=\"chat-goods__no-result-text\">{{:~t('chat.loadingProducts')}}</p> </div> {{/if}} </div> {{/if}} {^{if archiveCount > 0 && !~root.archiveLoaded}} <div class=\"chat-goods__infinite-scroll\" style=\"position:relative;\"> <div data-link=\"{infiniteScroll callback=~root.loadMoreArchive}\"></div> </div> {{/if}} </div> </div> {{else}} {^{include activeChat tmpl=activeChat^template /}} {{/if}} </div> </div> {{if ~root.currentLanguage != 'ru'}} <div class=\"text-reporter js-text-reporter\" data-link=\"{on 'click' ~root.openTextReporterMode}\" > <div                 class='text-reporter__wrap'> <span class='text-reporter__pencil'></span> <span> {{:~t('utils.nativeReview.pressToReport')}} </span> <span class='text-reporter__combination'> {{:isMacOS() ? '⌘ command + enter' : 'Ctrl + Enter'}} </span> </div> </div> {{/if}} <div tabindex=\"0\"></div> </section>"
    }
  }
